<table style="width: 100%; border: 1px;">
  <tr>
    <th>Date</th>
    <th>Home Team</th>
    <th></th>
    <th>Away Team</th>
    <th></th>
  </tr>
  <%= for game <- @games do %>
    <tr>
      <td><%= game.date %></td>
      <td><%= game.hometeam %></td>
      <td><%= game.fthg %></td>
      <td><%= game.awayteam %></td>
      <td><%= game.ftag %></td>
    </tr>
  <% end %>
</table>